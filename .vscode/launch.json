    {
      "name": "Python Debugger: t2",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test/t2.py",
      "cwd": "${workspaceFolder}/test",
      "console": "integratedTerminal"
    }
  ]
}